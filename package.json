{
  "name": "graphql-jsonplaceholder",
  "version": "0.0.1",
  "description": "A GraphQL implementation of the JSONPlaceholder API",
  "keywords": [
    "typescript"
  ],
  "author": "Tobias Fuhrimann",
  "license": "MIT",
  "scripts": {
    "start": "node dist",
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "build": "rimraf dist && tsc",
    "lint": "tslint --fix 'src/**/*.ts'",
    "test": "npm run lint"
  },
  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/express-graphql": "^0.6.1",
    "@types/graphql": "^0.13.1",
    "@types/node": "^10.1.4",
    "@types/node-fetch": "^2.1.0",
    "dataloader": "^1.4.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "node-fetch": "^2.1.2"
  },
  "devDependencies": {
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.5",
    "tslint": "^5.10.0",
    "typescript": "^2.8.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mastertinner/nodejs-typescript-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mastertinner/nodejs-typescript-boilerplate/issues"
  },
  "engines": {
    "node": ">=8"
  }
}

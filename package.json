{
  "name": "graphql-jsonplaceholder",
  "version": "0.0.1",
  "description": "A GraphQL implementation of the JSONPlaceholder API",
  "keywords": [
    "typescript"
  ],
  "author": "Tobias Fuhrimann",
  "license": "MIT",
  "scripts": {
    "start": "node dist",
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "build": "rimraf dist && tsc",
    "lint": "tslint --fix 'src/**/*.ts'",
    "test": "npm run lint"
  },
  "dependencies": {
    "@types/express": "^4.0.37",
    "@types/express-graphql": "0.0.34",
    "@types/graphql": "^0.11.4",
    "@types/node": "^8.0.28",
    "@types/node-fetch": "^1.6.7",
    "dataloader": "^1.3.0",
    "express": "^4.15.4",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.3",
    "node-fetch": "^1.7.3"
  },
  "devDependencies": {
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mastertinner/nodejs-typescript-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mastertinner/nodejs-typescript-boilerplate/issues"
  },
  "engines": {
    "node": ">=8"
  }
}
